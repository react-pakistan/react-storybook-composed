{
  "name": "@react-pakistan/react-storybook-composed",
  "version": "1.0.0",
  "description": "Boilerplate template for React Storybook with TS and more...",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "types": "index.d.tsx",
  "scripts": {
    "build": "yarn clean && yarn build:ts && cp package.json lib && cp README.md lib",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build:ts": "tsc --project tsconfig.build.json",
    "build:ts:cjs": "tsc --project tsconfig.build-cjs.json",
    "clean": "yarn rimraf ./lib",
    "deploy-storybook": "storybook-to-ghpages",
    "lint": "npm-run-all --parallel lint:**",
    "lint-staged": "lint-staged",
    "lint:css": "stylelint './src/**/*.tsx'",
    "lint:tools": "tslint '.storybook/*.js'",
    "lint:ts": "tslint --project tsconfig.json --exclude '*/*icons/*'",
    "lint:tsc": "yarn tsc --noEmit",
    "publish:github": "cd ./lib && npm publish && cd ..",
    "publish:npm": "npm publish --access public",
    "screenshot": "npx storycap http://localhost:9001",
    "start": "start-storybook -p 9001 -c .storybook",
    "test": "jest --coverage --config jest/config.json",
    "upgrade": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-pakistan/react-storybook-composed.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "keywords": [],
  "author": "Taimoor Khan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-pakistan/react-storybook-composed/issues"
  },
  "homepage": "https://github.com/react-pakistan/react-storybook-composed#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-a11y": "^6.0.13",
    "@storybook/addon-actions": "^6.0.13",
    "@storybook/addon-backgrounds": "^6.0.13",
    "@storybook/addon-docs": "^6.0.13",
    "@storybook/addon-essentials": "^6.0.13",
    "@storybook/addon-google-analytics": "^6.0.13",
    "@storybook/addon-info": "^6.0.0-alpha.2",
    "@storybook/addon-knobs": "^6.0.13",
    "@storybook/addon-links": "^6.0.13",
    "@storybook/addon-notes": "^6.0.0-alpha.6",
    "@storybook/addon-storysource": "^6.0.13",
    "@storybook/addon-viewport": "^6.0.13",
    "@storybook/react": "^6.0.13",
    "@storybook/storybook-deployer": "^2.8.6",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/invariant": "^2.2.33",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-powerplug": "^1.0.0",
    "react-share": "^4.2.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.3",
    "storybook-readme": "^5.0.8",
    "storycap": "^2.3.5",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "tslint": "^6.1.3",
